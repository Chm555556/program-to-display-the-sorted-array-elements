#include<stdio.h>
#include<conio.h>
void main()
{
int a[7],i,j,temp;
printf("Enter the elements of an array");
for(i=0;i<5;i++)
{
scanf("%d",&a[i]);
}
for(j=0;j<4;j++)
{
for(i=0;i<4-j;j++)
{
if(a[i]>a[i+1])
{
temp=a[i];
a[i]=a[i+1];
a[i+1]=temp;
}
}
}
printf("sorted elements are /n");
for(i=0;i<5;i++)
{
printf("%d",a[i]);
}
getch();
}
